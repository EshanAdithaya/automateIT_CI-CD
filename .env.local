# Local Development Environment
NODE_ENV=development
PORT=3000
APP_URL=http://localhost:3000

# GitHub Configuration (Add your tokens)
GITHUB_TOKEN=
GITHUB_WEBHOOK_SECRET=development_webhook_secret

# GitLab Configuration (Add your tokens)
GITLAB_TOKEN=
GITLAB_WEBHOOK_SECRET=development_gitlab_webhook

# Development Settings
AUTO_DEPLOY_ENABLED=false
DEFAULT_BUILD_TIMEOUT=1800
DEFAULT_TEST_TIMEOUT=600

# Security (Generate secure keys for production)
JWT_SECRET=dev_jwt_secret_key_change_in_production
ENCRYPTION_KEY=dev_encryption_key_change_in_production